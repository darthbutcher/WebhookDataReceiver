BOT_TOKENS[] =
BOT_TOKENS[] =
BOT_TOKENS[] =
BOT_TOKENS[] =
BOT_TOKENS[] =
BOT_TOKENS[] =
BOT_TOKENS[] =
BOT_TOKENS[] =

[QUEST]
Subscriptions = ENABLED
Discord_Feeds = ENABLED

# Default quest alert delivery time.
Default_Delivery = 09:00

